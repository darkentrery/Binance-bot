[Unit]
Description=gunicorn daemon
Requires=gunicorn_parser.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/Binance-bot
Environment="PATH=/home/ubuntu/Binance-bot/venv/bin"
ExecStart=/home/ubuntu/summarizer/venv/bin/python3 /home/ubuntu/Binance-bot/run.py
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target